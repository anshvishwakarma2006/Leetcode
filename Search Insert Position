class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        low,high=0,len(nums)-1
        while low<=high:
            mid=(low+high)//2
            if nums[mid]==target:
                return mid
            elif nums[mid]<target:
                low=mid+1
            else:
                high=mid-1
        if target not in nums:
            x=target-1
            if x in nums:
                z=nums.index(x)
                return z+1
            else:
                x-=1    
